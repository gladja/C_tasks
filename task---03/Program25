using System;

namespace task_01
{
    class Program
    {
        static void Main()
        {

            double n = double.Parse(Console.ReadLine());
            double sum = 0;

            for (int i = 0; n >= sum; i++)
            {
                sum = Math.Pow(2, i);
                if (sum > n)
                {
                    break;
                }
                Console.Write($"{sum} ");
            }
        }
    }
}